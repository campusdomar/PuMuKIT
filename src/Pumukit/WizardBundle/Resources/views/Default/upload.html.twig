<script type="text/javascript">
  var showSeries = "{{ show_series }}";
  var option = "{{ option }}";
  var uploaded = '{{ uploaded }}';
  var seriesId = "{{ seriesId }}";
  var sameSeries = "{{ same_series }}";
  if ('success' == uploaded){
    if ('single' == option){
      var mmId = "{{ mmId }}";
      var route = "{{ path('pumukitwizard_default_end', {'seriesId': '__seriesId__', 'mmId': '__mmId__', 'option': '__option__', 'show_series': '__showSeries__', 'same_series': '__sameSeries__'})|raw }}";
      route = route.replace('__mmId__', mmId);
    }else{
      var route = "{{ path('pumukitwizard_default_end', {'seriesId': '__seriesId__', 'option': '__option__', 'show_series': '__showSeries__', 'same_series': '__sameSeries__'})|raw }}";
    }
    route = route.replace('__seriesId__', seriesId);
    route = route.replace('__option__', option);
    route = route.replace('__showSeries__', showSeries);
    route = route.replace('__sameSeries__', sameSeries);
    parent.$('#pumukitwizard_body').load(route, {'_method':'POST'});
  }else{
    var message = "{{ message }}";
    var route = "{{ path('pumukitwizard_default_error', {'seriesId': '__seriesId__', 'option': '__option__', 'show_series': '__showSeries__', 'same_series': '__sameSeries__'})|raw }}";
    route = route.replace('__option__', option);
    route = route.replace('__showSeries__', showSeries);
    route = route.replace('__seriesId__', seriesId);
    route = route.replace('__sameSeries__', sameSeries);
    parent.$('#pumukitwizard_body').load(route, {'errormessage': message, '_method': 'POST'});
    parent.no_success('{{ message }}');
  }
</script>
