{% for group in resource.getGroups() %}
<a href="#" class="list-group-item add-{{ resource_name }}-group-item pumukitnewadmin-trim-row"
   id="add_{{ resource_name }}_group_{{ group.id }}"
   onclick="javascript:return select_group(this);"
   ondblclick="javascript:delete_group('{{ group.id }}', '{{ group.key }}', '{{ group.name }}', '{{ group.origin }}', '{{ resource_name }}');">
  <i class="mdi-social-group"></i>
  {{ group.key }} - {{ group.name }} - ({{ group.origin }})
</a>
{% endfor %}
