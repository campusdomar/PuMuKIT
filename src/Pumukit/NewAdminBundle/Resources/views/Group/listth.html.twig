<input type="hidden" name="pumukitnewadmin_group_sort_change_type" id="pumukitnewadmin_group_sort_change_type" value="{{ app.session.get('admin/group/type')=='asc'?'desc':'asc' }}"/>
{% if 'key' == app.session.get('admin/group/sort') %}
