    <th id="th-checkbox" width="1%">
        <div class="checkbox">
            <label>
                <input type="checkbox" onclick="window.click_checkbox_all('tag_children', this.checked)">
            </label>
        </div>
    </th>
    <th id="th-icons" colspan="2" width="5%"></th>
    <th id="th-name">
        {% trans %}Name{% endtrans %}
    </th>
    <th id="th-num-video" width="1%" class="text-center">
        {% trans %}Videos{% endtrans %}
    </th>
</tr>