<div id="directo-sbs" style="width:100%; margin: 0px auto;"></div>

<script type="text/javascript">
$(function(){
  jwplayer("directo-sbs").setup({
      file: "{{ generate_hls_url(live) }}",
      autostart: true,
      width: "100%",
      stretching: "exactfit",
      aspectratio: "16:9",
      logo: {hide: true},
      analytics: {enabled: false},
      androidhls: true,
      hlshtml: true,
      primary: "html5"
  });
});

</script>
