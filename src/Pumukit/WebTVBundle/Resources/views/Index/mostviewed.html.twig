<div class="col-no-padding col-lg-6 col-md-6 col-sm-6">
    <div class="row">
        <div class="col-xs-12 most-viewed">
            {% if show_last_month %}
                <h2>{% trans %} Most viewed since last month {% endtrans %}</h2>
            {% else %}
                <h2>{% trans %} Most viewed {% endtrans %}</h2>
            {% endif %}
        </div>
    </div>
    <div class="row">
        <div class="view-content">
            {% for object in multimediaObjectsSortedByNumview %}
                <!--Objeto multimedia-->
                {% include 'PumukitWebTVBundle:Misc:multimediaobject.html.twig' with {'rows':1} %}
            {% endfor %}
        </div>
    </div>
</div>
