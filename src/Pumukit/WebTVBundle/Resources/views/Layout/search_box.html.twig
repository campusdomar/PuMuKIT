<!--//SEARCH BOX-->
<div class="search-box">
    <form class="input-group stylish-input-group" method="get" action="{{ path('pumukit_webtv_search_multimediaobjects') }}">
        <label for="search-box-{{ id|default('base') }}" class="sr-only">
            {% trans %}Search{% endtrans %}
        </label>
        <input type="text" class="form-control" placeholder="{% trans %} Search {% endtrans %}..." name="search" id="search-box-{{ id|default('base') }}" />
        <span class="input-group-addon">
            <label for="submit-button-{{ id|default('base') }}" class="sr-only">{% trans %}Submit{% endtrans %}</label>
            <button type="submit" id="submit-button-{{ id|default('base') }}" style="border:0;background:transparent;" title="{% trans %}Submit{% endtrans %}">
                <span class="glyphicon glyphicon-search"></span>
            </button>
        </span>
    </form>
</div>
