{
    "name": "pumukit/pumukit2",
    "version": "3.0.x-dev",
    "license": "GPL",
    "type": "project",
    "description": "Media Portal",
    "require": {
        "php": "^7.0.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alcaeus/mongo-php-adapter": "@stable",
        "doctrine/mongodb-odm-bundle": "^3.5",
        "friendsofsymfony/user-bundle": "^2.1",
        "jasig/phpcas": "^1.3",
        "jms/serializer-bundle": "^3.1",
        "knplabs/knp-menu-bundle": "^2.2",
        "sensio/framework-extra-bundle": "^3.0.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "suncat/mobile-detect-bundle": "^1.1",
        "symfony/asset": "3.4.*",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/expression-language": "3.4.*",
        "symfony/flex": "^1.1",
        "symfony/form": "3.4.*",
        "symfony/framework-bundle": "3.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "3.4.*",
        "symfony/security-bundle": "3.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "3.4.*",
        "symfony/translation": "3.4.*",
        "symfony/twig-bundle": "3.4.*",
        "symfony/validator": "3.4.*",
        "symfony/web-link": "3.4.*",
        "symfony/yaml": "3.4.*",
        "teltek/pmk2-stats-ui-bundle": "^1.0",
        "twig/extensions": "^1.5",
        "vipx/bot-detect-bundle": "^4.0",
        "white-october/pagerfanta-bundle": "^1.2"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "3.4.*"
    },
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Pumukit\\": "src/Pumukit/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pumukit\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    }
}
